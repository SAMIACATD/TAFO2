
@{
    ViewBag.Title = "Cadastro";
}


<h2>Cadastro de Alimentos</h2>

<!-- Primeira Linha-->
<div class="form-group row">
    <div class="col-sm-8">
        <label>Razão Social</label>
        <input type="text" id="razao-social" class="form-control" MaxLength="15" />
    </div>

    <div class="col-sm-2">
        <label>Inscrição Estadual</label>
        <input type="text" id="ie" class="form-control" />
    </div>
    <div class="col-sm-2">
        <label>CNPJ</label>
        <input type="text" id="cnpj" class="form-control" />
    </div>
</div>
<!-- Segunda Linha-->
<div class="form-group row">
    <div class="col-sm-6">
        <label>Fantasia</label>
        <input type="text" id="fantasia" class="form-control" MaxLength="15" />
    </div>

    <div class="col-sm-2">
        <label>Email</label>
        <input type="text" id="email" class="form-control" placeholder="nome@exemplo.com" />
    </div>
    <div class="col-sm-2">
        <label>Fax</label>
        <input type="text" id="fax" class="form-control" />
    </div>
    <div class="col-sm-2">
        <label>Logo</label>
        <input type="text" id="logo" class="form-control" />
    </div>
